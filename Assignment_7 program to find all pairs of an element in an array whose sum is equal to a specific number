list=[]
n=int(input("enter string in this list")) 
for i in range(n) :
  a=int(input (" enter a string ")) 
  list.append(a) 
print("the list is", list)
str=int(input ("enter a value:")) 
s1=0
s2=len(list)-1
while(s1<=s2):
 if(list[s1]+list[s2]>str):
   s2=s2-1
 elif(list[s1]+list[s2]<str):
   s1=s2+1
 elif(list[s1]+list[s2]==str):
 print("the sum of pairs are", list[s1], list[s2]) 
 s2=s2-1
 s1=s1+1
